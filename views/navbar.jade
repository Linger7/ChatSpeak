extends layout

block navbar
	nav.navbar.navbar-default.navbar-fixed-top#navbarContainer
		div.container
				div.navbar-header
					button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href='#')
						img(src='images/logos/logo.png' id='navbarLogo')
				div#navbar.navbar-collapse.collapse
					ul.nav.navbar-nav.navbar-right
						li.active#navbarItemOne
							if isAuthed == true
								a(href='#' onclick='ajaxModalCall("accounts")') Profile
							else
								a(href='#' onclick='ajaxModalCall("accounts")') Login